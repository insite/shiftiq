		<rules>

			<rule name="Block AppSettings" stopProcessing="true">
				<match url=".*AppSettings.*\.json$" />
				<action type="CustomResponse" statusCode="403" statusReason="Forbidden" statusDescription="Access denied" />
			</rule>
			
			<!-- Fixes -->

			<rule name="DEV-9392 Replace employee with userID (A)" stopProcessing="true">
				<match url="^ui/cmds/design/validations/competencies/search$" />
				<conditions>
					<add input="{QUERY_STRING}" pattern="employee=(.+)" />
				</conditions>
				<action type="Redirect" url="ui/cmds/design/validations/competencies/search?userID={C:1}" appendQueryString="false" redirectType="SeeOther" />
			</rule>
			<rule name="DEV-9392 Replace employee with userID (B)" stopProcessing="true">
				<match url="^ui/cmds/portal/validations/competencies/validate$" />
				<conditions>
					<add input="{QUERY_STRING}" pattern="id=(.+)&amp;employee=(.+)&amp;keepWorkingSet=yes" />
				</conditions>
				<action type="Redirect" url="ui/cmds/portal/validations/competencies/validate?id={C:1}&amp;userID={C:2}&amp;keepWorkingSet=yes" appendQueryString="false" redirectType="SeeOther" />
			</rule>
			<rule name="Assets: Fix old program launch links" stopProcessing="true">
				<match url="^asset/(launch|program)/(.+)$" />
				<action type="Redirect" url="user/asset/{R:1}/{R:2}" redirectType="SeeOther" />
			</rule>
			<rule name="Courses: Fix old course learn links 1" stopProcessing="true">
				<match url="^course/learn/(.+)$" />
				<action type="Redirect" url="ui/portal/courses/learn/{R:1}" redirectType="SeeOther" />
			</rule>
			<rule name="Courses: Fix old course learn links 2" stopProcessing="true">
				<match url="^ui/portal/courses/learn/(.+)$" />
				<action type="Redirect" url="ui/portal/learning/course/{R:1}" redirectType="SeeOther" />
			</rule>
			<rule name="Courses: Fix old SCORM launch links" stopProcessing="true">
				<match url="^course/scorm/(.+)$" />
				<action type="Redirect" url="ui/portal/integrations/scorm/launch/{R:1}" redirectType="SeeOther" />
			</rule>
			<rule name="Messages: Fix old message links" stopProcessing="true">
				<match url="^(user/link/navigate|admin/messages/link|user/messages/link)$" />
				<action type="Redirect" url="/ui/lobby/messages/links/redirect" appendQueryString="true" />
			</rule>
			<rule name="Messages: Fix old message clicks" stopProcessing="true">
				<match url="^user/links/click$" />
				<action type="Redirect" url="/ui/lobby/messages/links/click" appendQueryString="true" />
			</rule>
			<rule name="Sites: Old Accounts" stopProcessing="true">
				<match url="^ui/(launch|site)/accounts/(register|reset-password|signin|signout|verify-email)$" />
				<action type="Redirect" url="/ui/lobby/{R:2}" appendQueryString="true" />
			</rule>
			<rule name="Sites: Old Subscribe" stopProcessing="true">
				<match url="^ui/site/contacts/subscribe$" />
				<action type="Redirect" url="/ui/lobby/subscribe" appendQueryString="true" />
			</rule>
			<rule name="Surveys: Old Survey Response Search" stopProcessing="true">
				<match url="^ui/portal/surveys/responses/search$" />
				<action type="Redirect" url="/ui/survey/respond/search" appendQueryString="false" />
			</rule>
			<rule name="Surveys: Old Survey Response Start" stopProcessing="true">
				<match url="^surveys/(.+)$" />
				<action type="Redirect" url="/survey/{R:1}" appendQueryString="true" />
			</rule>
			<rule name="Plugin: Old ITA Exam Authentication" stopProcessing="true">
				<match url="^custom/ita/exams/authenticate$" />
				<action type="Redirect" url="/ui/lobby/events/login" appendQueryString="true" />
			</rule>
			<rule name="Workflow: Old Issue Outline" stopProcessing="true">
				<match url="^ui/admin/issues/outline$" />
				<action type="Redirect" url="/ui/admin/workflow/cases/outline" appendQueryString="true" />
			</rule>

			<!-- API Changes -->

			<rule name="API: mvc/events/CreateExamScrapPaper" stopProcessing="true">
				<match url="^mvc/events/CreateExamScrapPaper" />
				<action type="Rewrite" url="/api/events/scrap-paper" />
			</rule>

			<!-- Plugins (customer-specific variants) -->

			<rule name="CAF" stopProcessing="true">
				<match url="^(ui/desktops/custom/caf/youthassessment)|youthassessment" />
				<conditions>
					<add input="{HTTP_HOST}" pattern="caf\.insite\.com$" />
				</conditions>
				<action type="Redirect" url="https://caf-trades.insite.com/" appendQueryString="false" />
			</rule>

			<!-- Portal/Assessments/Attempts -->

			<rule name="Redirect: ui/portal/assessments/attempts/start/{assessment}" stopProcessing="true">
				<match url="^ui/portal/assessments/attempts/start/([0-9a-f-]+)$" />
				<action type="Redirect" url="/ui/portal/assessments/attempts/start?resource={R:1}" redirectType="SeeOther" appendQueryString="true" />
			</rule>
			<rule name="Redirect: ui/portal/assessments/attempts/start/{course}/{assessment}" stopProcessing="true">
				<match url="^ui/portal/assessments/attempts/start/([0-9a-f-]+)/([0-9a-f-]+)$" />
				<action type="Redirect" url="/ui/portal/assessments/attempts/start?course={R:1}&amp;lesson={R:2}" redirectType="SeeOther" appendQueryString="true" />
			</rule>

			<!-- Defaults -->

			<rule name="Force SSL Connections" enabled="true" stopProcessing="true">
				<match url="(.*)" />
				<conditions>
					<add input="{HTTPS}" pattern="^OFF$" />
				</conditions>
				<action type="Redirect" url="https://{HTTP_HOST}/{R:1}" redirectType="SeeOther" />
			</rule>

			<rule name="Static Content" stopProcessing="true">
				<match url="^(cmds/(content|downloads))|library/" />
				<action type="None" />
			</rule>

			<rule name="Static Content (errors: html)" stopProcessing="true">
				<match url="^(400)$" />
				<action type="Rewrite" url="/ui/lobby/logs/{R:1}.html" />
			</rule>

			<rule name="Static Content (errors: aspx)" stopProcessing="true">
				<match url="^(403|404|410|500)$" />
				<action type="Rewrite" url="/ui/lobby/logs/{R:1}.aspx" />
			</rule>
			
			<rule name="Static Content (files)" stopProcessing="true">
				<match url="^files/(.+)$" />
				<action type="Rewrite" url="/Web/Persistence/DownloadFile.ashx?path={R:1}" />
			</rule>

			<rule name="Static Content (images)" stopProcessing="true">
				<match url="^images/(.+)$" />
				<action type="Rewrite" url="/library/images/{R:1}" />
			</rule>

			<rule name="Static Content (in-content)" stopProcessing="true">
				<match url="^in-content/(.+)$" />
				<action type="Rewrite" url="/Web/Persistence/DownloadFile.ashx?path={R:1}" />
			</rule>

			<rule name="Static Content (uploads)" stopProcessing="true">
				<match url="^uploads/(.+)$" />
				<action type="Rewrite" url="/Web/Persistence/DownloadFile.ashx?path={R:1}" />
			</rule>

			<rule name="Static Content (uploads for CMDS)" stopProcessing="true">
				<match url="^cmds/uploads/(.+)/(.+)$" />
				<action type="Rewrite" url="/Web/Persistence/DownloadFile.ashx?id={R:1}&amp;name={R:2}" />
			</rule>

			<rule name="Health/Status Check" stopProcessing="true">
				<match url="^ui/(health|status)$" />
				<action type="Rewrite" url="/Web/Persistence/AppStatus.ashx" />
			</rule>

			<rule name="Version" stopProcessing="true">
				<match url="^ui/version" />
				<action type="Rewrite" url="/Web/Persistence/AppStatus.ashx" />
			</rule>
			
			<rule name="Refer to Routing.Configuration (1 of 2)" stopProcessing="true">
				<match url="^(admin|cmds|course|error|help|lti|portals|survey|ui)/" />
				<action type="None" />
			</rule>

			<rule name="Refer to Routing.Configuration (2 of 2)" stopProcessing="true">
				<match url="^(custom/ita/exams/authenticate)" />
				<action type="None" />
			</rule>

			<!-- Sites -->

			<rule name="Sign In" stopProcessing="true">
				<match url="^(?:(portal|user)/(account|users)/signin)?$|^login$" />
				<action type="Redirect" url="/ui/lobby/signin" appendQueryString="false" redirectType="SeeOther" />
			</rule>

			<!-- Portal/Progressions -->

			<rule name="Redirect: portal/progressions/reports/certificate/{id}/{user}">
				<match url="^portal/progressions/reports/certificate/([0-9a-z-]+)/([0-9a-z-]+)$" />
				<action type="Rewrite" url="/ui/portal/records/credentials/certificate?id={R:1}&amp;user={R:2}" redirectType="SeeOther" appendQueryString="true" />
			</rule>

			<rule name="Redirect: portal/progressions/reports/certificate/{id}" stopProcessing="true">
				<match url="^portal/progressions/reports/certificate/([0-9a-z-]+)?$" />
				<action type="Redirect" url="/ui/portal/records/credentials/certificate?id={R:1}" redirectType="SeeOther" appendQueryString="true" />
			</rule>

		</rules>
